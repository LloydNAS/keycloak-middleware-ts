{
  "name": "keycloak-backend",
  "version": "2.0.1",
  "description": "Keycloak Node.js minimalist connector for backend services integration.",
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "tsc --build --clean --dry",
    "build": "tsc --build",
    "test": "node japaFile.js",
    "lint": "ts-standard",
    "format": "ts-standard --fix",
    "coverage": "nyc npm run test"
  },
  "repository": {
