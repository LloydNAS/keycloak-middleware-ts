{
  "name": "keycloak-middleware-ts",
  "version": "1.1.3",
  "description": "Keycloak minimalist middleware connector for backend services.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "prepare": "npm run build",
    "build": "tsc --build",
    "lint": "ts-standard",
    "format": "ts-standard --fix",
    "test": "node japaFile.js",
    "test:cov": "nyc --reporter=lcov npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rafaelcascalho/keycloak-middleware"
  },
  "keywords": [
    "keycloak",
    "auth",
    "oauth",
    "jwt",
    "rest",
    "typescript",
    "ts"
  ],
  "files": [
    "LICENSE",
    "README.md",
    "lib/",
    "!test/*.spec.ts"
  ],
  "author": {
    "name": "Rafael Cascalho <cascalhoeng@gmail.com>",
    "email": "cascalhoeng@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rafaelcascalho/keycloak-middleware/issues"
  },
  "homepage": "https://github.com/rafaelcascalho/keycloak-middleware#readme",
  "dependencies": {
    "axios": "^0.21.0",
    "jsonwebtoken": "^8.5.1"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^8.5.6",
    "japa": "^4.0.0",
    "nyc": "^15.1.0",
    "standard": "^16.0.4",
    "ts-node": "^10.4.0",
    "ts-standard": "^11.0.0",
    "typescript": "^4.5.2"
  },
  "ts-standard": {
    "ignore": [
      "dist"
    ],
    "envs": [
      "node"
    ],
    "report": "stylish",
    "extensions": ".ts,.js"
  }
