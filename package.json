    "auth",
    "oauth",
    "jwt",
    "rest"
  ],
  "files": [
    "LICENSE",
    "README.md",
    "src/"
  ],
  "author": "Rafael Cascalho <cascalhoeng@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jkyberneees/keycloak-backend/issues"
  },
  "homepage": "https://github.com/jkyberneees/keycloak-backend#readme",
  "dependencies": {
    "axios": "^0.21.0",
    "jsonwebtoken": "^8.5.1"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^8.5.6",
    "japa": "^4.0.0",
    "standard": "^15.0.0",
    "ts-node": "^10.4.0",
    "ts-standard": "^11.0.0",
    "typescript": "^4.5.2"
  },
  "ts-standard": {
    "ignore": [
      "dist"
    ],
    "envs": [
      "node"
    ],
